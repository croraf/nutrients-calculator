    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux-form": "^8.3.0",
    "autosuggest-highlight": "^3.1.0",
    "chart.js": "^1.1.1",
    "connected-react-router": "^6.8.0",
    "history": "^4.7.2",
    "react": "^17.0.1",
    "react-autosuggest": "^9.3.1",
    "react-big-calendar": "^0.14.4",
