{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "commonjs": true,
        "es6": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        },
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "plugins": [
    ],
    "settings": {
    },
    "rules": {
        "strict": "error",
        "indent": ["error", 2, {"SwitchCase": 1}],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off"
    }
}
